{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "sort": false,
        "template": "node_modules/ink-docstrap/template",
        "destination": "docs/",
        "recurse": true,
        "tutorials": "src/docs/tutorials"
    },
    "plugins": ["plugins/staticFiles"],
    "templates": {
      "logoFile": "static/logo.svg",
      "cleverLinks": true,
      "monospaceLinks": false,
      "dateFormat": "ddd MMM Do YYYY",
      "outputSourceFiles": true,
      "outputSourcePath": true,
      "systemName": "QkApp",
      "footer": "",
      "copyright": "(c) Abhay Bhave - 2018 - Gadre InfoTech Pvt. Ltd. (IND)",
      "navType": "inline",
      "theme": "Paper",
      "linenums": true,
      "collapseSymbols": false,
      "inverseNav": true,
      "protocol": "html://",
      "methodHeadingReturns": false,
      "search": false
    },
    "markdown": {
      "parser": "gfm",
      "hardwrap": true
    },
    "staticFiles": {
       "include": ["static:src/docs/static"],
       "exclude": [],
       "includePattern": ".+\\.(png|jpg|svg)$",
       "excludePattern": "(^|\\/|\\\\)_",
       "recursive": true,
       "recursiveLevel": 2
    }
}
